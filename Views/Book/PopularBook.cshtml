@model IEnumerable<Lab03.Models.Book>;
@foreach(var item in Model)
{
	<div class="col-xs-3 col-sm-3 col-md-3 col-lg-3">
		<div class="thumbnail">
			<img src="@item.ImageUrl" alt="" style="height:50%;width=50%"/>
			<div class="caption">
				<h3>@item.Title</h3>
				<p>
					Price: @item.Price
				</p>
			</div>
		</div>
	</div>
}
